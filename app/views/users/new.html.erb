<% @user.errors.full_messages.each do |error| %>
  <li><%= error %></li>
<% end %>


<%= form_for @user do |form| %>

<h1>Create Account</h1>
 <table>
    <tr>
      <td><%= form.label :username %></td>
      <td><%= form.text_field :username %></td>
    </tr>
    <tr>
      <td><%= form.label :password%></td>
      <td><%= form.password_field :password_digest %> </td>
    </tr>
    <tr>
      <td><%= form.label :first_name %>
      <td><%= form.text_field :first_name %></td>
    </tr>
    <tr>
      <td><%= form.label :last_name %></td>
      <td><%= form.text_field :last_name %></td>
    <tr>
      <td><%= form.label :email %></td>
      <td><%= form.text_field :email %></td>
    </tr
    <tr>
      <td><%= form.submit "Create" %> </td>
    </tr>
  </table>

<% end %>
	